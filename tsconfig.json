{
  "include": ["src/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "declaration": true,
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "target": "ES2017",
    "lib": ["DOM", "ES2017"]
  }
}
